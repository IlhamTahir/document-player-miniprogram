<view class="document-player">
    <view class="pages">
        <image bind:src="{{ item.loadSrc }}" wx:for="{{imageList}}" mode="aspectFit" data-src="{{item.src}}" wx:key="{{item.page}}" class="{{ item.active ? 'active' : '' }} {{ !item.load ? 'placeholder':''  }}}"></image>
    </view>
    <view class="toolbar" bind:page-changed="helloFunc">{{position.page}}</view>
</view>
